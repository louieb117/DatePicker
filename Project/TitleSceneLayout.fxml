<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>

<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javefx.com/fxml"
           fx:controller="Project.DatePickerController">

    <padding>
        <Insets top="205" bottom="205" right="485" left="485"/>
    </padding>

    <ImageView fitWidth="1080" preserveRatio="true" cache="true">
        <Image url="/Project/resources/art.jpg" />
    </ImageView>

    <Rectangle x="383" y="175" width="300" height="150" fill="#808080"/>

    <VBox spacing="55" alignment="CENTER">
        <Label text="Where To?" font="20" textFill="white" />
        <Button fx:id="welcomeButton" text="Welcome" mnemonicParsing="false" onAction="#handleWelcomeButtonAction"/>
    </VBox>

</StackPane>



